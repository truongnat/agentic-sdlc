{
  "name": "agentic-sdlc",
  "version": "1.0.1",
  "description": "Simulating a complete Software Development Lifecycle (SDLC) with specialized AI Agents.",
  "type": "module",
  "bin": {
    "agentic-sdlc": "./bin/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "workflow:cycle": "python .agent/scripts/workflows/cycle.py",
    "workflow:housekeeping": "python .agent/scripts/workflows/housekeeping.py",
    "kb:search": "python .agent/scripts/kb/search.py",
    "kb:stats": "python .agent/scripts/kb/stats.py",
    "kb:update": "python .agent/scripts/kb/update-index.py",
    "health": "python .agent/scripts/validation/health-check.py",
    "agent": "python .agent/scripts/run.py"
  },
  "keywords": [
    "sdlc",
    "ai",
    "agent",
    "automation",
    "workflow",
    "cursor",
    "copilot",
    "development",
    "project-management"
  ],
  "author": "truongnat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truongnat/agentic-sdlc.git"
  },
  "bugs": {
    "url": "https://github.com/truongnat/agentic-sdlc/issues"
  },
  "homepage": "https://github.com/truongnat/agentic-sdlc#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    ".agent/",
    "docs/",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}